alt - e : yabai -m space --layout bsp
alt - s : yabai -m space --layout stack

alt - j : yabai -m window --focus stack.next || yabai -m window --focus south
alt - k : yabai -m window --focus stack.prev || yabai -m window --focus north
alt - h : yabai -m window --focus west
alt - l : yabai -m window --focus east

shift + alt - h : yabai -m window --warp west
shift + alt - j : yabai -m window --warp south || yabai -m window --swap stack.next
shift + alt - k : yabai -m window --warp north || yabai -m window --swap stack.prev
shift + alt - l : yabai -m window --warp east

alt - f : yabai -m window --toggle float
alt - space : yabai -m window --toggle split
